{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "tsc",
    "databaseProd": "tsc && pm2 start ./dist/api/index.js --name \"api-rest\" --time",
    "databaseDev": "ts-node ./src/api/index.ts",
    "processorProd": "tsc && pm2 start dist/data-processor/app/index.js --name \"data-processor\" --time -o out.log -e err.log",
    "processorDev": "ts-node ./src/data-processor/app/index.ts",
    "build": "tsc",
    "test": "jest --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^16.0.1",
    "express": "^4.17.3",
    "mongoose": "^6.2.10",
    "morgan": "^1.10.0",
    "pm2": "^5.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "preset": "ts-jest",
    "rootDir": ".",
    "testRegex": "\\..*spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "engines": {
    "node": "16.14"
  }
}
